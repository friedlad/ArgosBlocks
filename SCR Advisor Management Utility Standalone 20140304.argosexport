     &lt;Choices&gt;&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select  rownum as "RowId",&#xD;
        rset_final.*&#xD;
&#xD;
from    (&#xD;
        select  distinct&#xD;
                "Id",&#xD;
                "Name",&#xD;
                "RecType",&#xD;
                "Status",&#xD;
                "CloseoutTerm",&#xD;
                "MaxAdvrTerm",&#xD;
                --"StatusTerm",&#xD;
                "Levl",&#xD;
                "SType",&#xD;
                "LastReg",&#xD;
                decode(:cbIncludeAdvisorDataTerm, 0, '--', "Type") as "Type",&#xD;
                decode(:cbIncludeAdvisorDataTerm, 0, '--', "Advisor") as "Advisor",&#xD;
                decode(:cbIncludeAdvisorDataTerm, 0, '--', "AdvrTerm") as "AdvrTerm",&#xD;
                pidm&#xD;
&#xD;
        from    (&#xD;
                select  f_getspridenid(pidm) "Id",&#xD;
                        f_format_name(pidm,'LF30') as "Name",&#xD;
                        rectype as "RecType",&#xD;
                        status as "Status",&#xD;
                        term_code as "StatusTerm",&#xD;
                        max_advr_term as "MaxAdvrTerm",&#xD;
                        levl as "Levl",&#xD;
                        stype as "SType",&#xD;
                        (&#xD;
                        select max(sfrstcr_term_code)&#xD;
                        from sfrstcr, stvrsts&#xD;
                        where sfrstcr_pidm = pidm&#xD;
                        and sfrstcr_rsts_code = stvrsts_code&#xD;
                        and stvrsts_incl_sect_enrl = 'Y'&#xD;
                        ) as "LastReg",&#xD;
                        sgradvr_advr_code as "Type",&#xD;
                        f_format_name(sgradvr_advr_pidm,'LF30') AS "Advisor",&#xD;
                        sgradvr_term_code_eff as "AdvrTerm",&#xD;
                        case&#xD;
                            when max_advr_term = 999999 then 'issue you must remove adv rec with 999999'&#xD;
                            when closeout_term_code &amp;amp;lt;= max_advr_term then&#xD;
                                 fy_stvterm_next(max_advr_term)&#xD;
                            else&#xD;
                                closeout_term_code&#xD;
                        end as "CloseoutTerm",&#xD;
                        pidm&#xD;
&#xD;
                from    (&#xD;
                        --for students no longer associated here...&#xD;
                        select  distinct&#xD;
                                tdn.sgbstdn_pidm as pidm,&#xD;
                                'Inactive' as rectype,&#xD;
                                tdn.sgbstdn_stst_code as status,&#xD;
                                tdn.sgbstdn_term_code_eff as term_code,&#xD;
                                tdn.sgbstdn_levl_code as levl,&#xD;
                                tdn.sgbstdn_styp_code as stype,&#xD;
                                dvr.sgradvr_advr_code,&#xD;
                                dvr.sgradvr_advr_pidm,&#xD;
                                dvr.sgradvr_term_code_eff,&#xD;
                                tdn.sgbstdn_term_code_eff as closeout_term_code,&#xD;
                                dvr.sgradvr_term_code_eff as max_advr_term&#xD;
                        from    sgbstdn tdn,&#xD;
                                sgradvr dvr,&#xD;
                                stvstst stv&#xD;
&#xD;
                        where   tdn.rowid = f_get_sgbstdn_rowid(tdn.sgbstdn_pidm, tdn.sgbstdn_term_code_eff)&#xD;
                                  /* Get the most current record the standard way&#xD;
                                  tdn.sgbstdn_term_code_eff = (select max(n.sgbstdn_term_code_eff)&#xD;
                                                              from sgbstdn n&#xD;
                                                              where tdn.sgbstdn_pidm = n.sgbstdn_pidm)*/&#xD;
                          and   stv.STVSTST_CSTS_CODE = 'REMOVED'&#xD;
                          and   sgbstdn_stst_code = stv.STVSTST_CODE (+)&#xD;
                                /* sgbstdn_stst_code IN (select STVSTST_CODE&#xD;
                                                      from STVSTST&#xD;
                                                      where STVSTST_CSTS_CODE = 'REMOVED') -- SCR */&#xD;
                                /* Previously to determine 'REMOVED' Statuses&#xD;
                                   sgbstdn_stst_code IN ('GD','AK', 'DA', 'WD', 'IS', 'NS') */&#xD;
                        and     tdn.sgbstdn_pidm = dvr.sgradvr_pidm&#xD;
                        and     dvr.sgradvr_advr_pidm is not null&#xD;
                        and     dvr.sgradvr_term_code_eff = (select max(r.sgradvr_term_code_eff)&#xD;
                                                             from sgradvr r&#xD;
                                                             where r.sgradvr_pidm = dvr.sgradvr_pidm)&#xD;
                        and     not exists (&#xD;
                                   select *&#xD;
                                   from sfrstcr, stvrsts&#xD;
                                   where sfrstcr_pidm = tdn.sgbstdn_pidm&#xD;
                                   and sfrstcr_rsts_code = stvrsts_code&#xD;
                                   and stvrsts_incl_sect_enrl = 'Y'&#xD;
                                   and sfrstcr_term_code &amp;amp;gt;= (select fy_stvterm_current('STANDARD', 'SICAS_TERM_TYPE', 'NEXT') from dual)) --dbl_check that not currently registered&#xD;
&#xD;
                        UNION&#xD;
&#xD;
                        --This is for ACTIVE grad students previously under-grad.  we need to&#xD;
                        --close out the advisor record if that record belonged to the under-grad record.&#xD;
                        select  distinct&#xD;
                                stdn.sgbstdn_pidm as pidm,&#xD;
                                'Active Grad' as rectype,&#xD;
                                stdn.sgbstdn_stst_code as status,&#xD;
                                stdn.sgbstdn_term_code_eff as term_code,&#xD;
                                stdn.sgbstdn_levl_code as levl,&#xD;
                                stdn.sgbstdn_styp_code as stype,&#xD;
                                advr.sgradvr_advr_code,&#xD;
                                advr.sgradvr_advr_pidm,&#xD;
                                advr.sgradvr_term_code_eff,&#xD;
                                maxug.max_ug_term_code_eff as closeout_term_code,&#xD;
                                advr.sgradvr_term_code_eff as max_advr_term&#xD;
                        from    sgbstdn stdn,&#xD;
                                stvstst va,&#xD;
                                sgradvr advr,&#xD;
                                (select distinct n.sgbstdn_pidm,&#xD;
                                        min(n.sgbstdn_term_code_eff) over (partition by sgbstdn_pidm) as term_code_eff_min,&#xD;
                                        max(n.sgbstdn_term_code_eff) over (partition by sgbstdn_pidm) as term_code_eff_max&#xD;
                                from    sgbstdn n,&#xD;
                                        stvstst v&#xD;
                                where   n.SGBSTDN_STST_CODE = v.stvstst_code (+)&#xD;
                                  and   v.stvstst_csts_code is null -- campus non-specific&#xD;
                                          /* Previously to determine Active:&#xD;
                                             n.sgbstdn_stst_code = 'AS' */&#xD;
                                  and   n.sgbstdn_levl_code = :lbGraduateCodes.Level_Code&#xD;
                                          /* Previously (before Standalone version:&#xD;
                                             f_sz_isGradStudent(n.sgbstdn_levl_code, n.sgbstdn_pidm, n.sgbstdn_term_code_eff) = 1      -- the SCR way */&#xD;
                                          /* Previously to determine grad level:&#xD;
                                             n.sgbstdn_levl_code = 'GR'*/&#xD;
                                ) mdn, --this gets the min and max only for the GR record&#xD;
                                (select distinct d.sgbstdn_pidm,&#xD;
                                        max(sgbstdn_term_code_eff) as max_term_code_eff&#xD;
                                from    sgbstdn d&#xD;
                                group by d.sgbstdn_pidm&#xD;
                                )maxdm, --this gets the max overall, combined with the above we ensure we are dealing only w/ GR's as the last record&#xD;
                                (select distinct t.sgbstdn_pidm,&#xD;
                                        max(t.sgbstdn_term_code_eff) as max_ug_term_code_eff&#xD;
                                from    sgbstdn t&#xD;
                                where   t.sgbstdn_levl_code = :lbUndergradCodes.Level_Code&#xD;
                                          /* Previously (before Standalone version:&#xD;
                                             f_sz_isUndergrad(t.sgbstdn_levl_code, t.sgbstdn_pidm, t.sgbstdn_term_code_eff) = 1      -- the SCR way */&#xD;
                                          /* Previously to determine undergrad level:&#xD;
                                             t.sgbstdn_levl_code = 'UG' */&#xD;
                                group by t.sgbstdn_pidm&#xD;
                                )maxug --making sure a UG record existed before date will be used because sometimes a record exists in&#xD;
                                       --advr table where the date is between the last UG and 1st GR (eg pidm: 7420)&#xD;
&#xD;
                                where   stdn.sgbstdn_pidm = mdn.sgbstdn_pidm&#xD;
                                and     stdn.sgbstdn_pidm = maxdm.sgbstdn_pidm&#xD;
                                and     stdn.sgbstdn_pidm = advr.sgradvr_pidm&#xD;
                                and     stdn.SGBSTDN_STST_CODE = va.stvstst_code (+)&#xD;
                                  and   va.stvstst_csts_code is null -- campus non-specific&#xD;
                                          /* Previously to determine Active:&#xD;
                                             stdn.sgbstdn_stst_code = 'AS'*/&#xD;
                                  and   stdn.sgbstdn_levl_code = :lbGraduateCodes.Level_Code&#xD;
                                          /* Previously (before Standalone version:&#xD;
                                             f_sz_isGradStudent(stdn.sgbstdn_levl_code, stdn.sgbstdn_pidm, stdn.sgbstdn_term_code_eff) = 1      -- the SCR way&#xD;
                                          /* Previously to determine grad level:&#xD;
                                             stdn.sgbstdn_levl_code = 'GR' */&#xD;
                                and     stdn.sgbstdn_term_code_eff = mdn.term_code_eff_max&#xD;
                                and     stdn.sgbstdn_term_code_eff = maxdm.max_term_code_eff&#xD;
                                and     stdn.sgbstdn_pidm = maxug.sgbstdn_pidm&#xD;
                                and     advr.sgradvr_term_code_eff = (select max(dvr.sgradvr_term_code_eff) from sgradvr dvr where dvr.sgradvr_pidm = advr.sgradvr_pidm)&#xD;
                                and     advr.sgradvr_advr_pidm is not null&#xD;
                                and     advr.sgradvr_term_code_eff &amp;amp;lt;= maxug.max_ug_term_code_eff&#xD;
&#xD;
                        )&#xD;
&#xD;
                where   (term_code = :ddTermTerm.Term)&#xD;
                  and   (:txtBannerIdTerm is null or :sql_SelByBannerId.selBannerId = pidm)&#xD;
                  and   (:cbStatus = 0 or status = :lbStatus.Status_Code)&#xD;
                  and   (:cbLevl = 0 or levl = :lbLevl.Level_Code)&#xD;
                order by&#xD;
                        "Name"&#xD;
                ) rset&#xD;
        --where   :buttGetRecords is not null&#xD;
        order by&#xD;
                2&#xD;
        ) rset_final&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;&lt;/Choices&gt;&#xD;
