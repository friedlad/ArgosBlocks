     &lt;Choices&gt;&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select&#xD;
--first do the default term&#xD;
stvterm_code "code",&#xD;
stvterm_code || ' - ' || stvterm_desc "description",&#xD;
1 sortFlag&#xD;
from stvterm&#xD;
where stvterm_code = fy_stvterm_next(fy_stvterm_current('STANDARD', 'SICAS_TERM_TYPE', 'NEXT'), 'STANDARD')&#xD;
&#xD;
-- now lets put in a blank row to separate default term from rest of list&#xD;
union select null, null, 2 from dual&#xD;
&#xD;
-- fill out rest of term list&#xD;
union select&#xD;
stvterm_code "code",&#xD;
stvterm_code || ' - ' || stvterm_desc "desc",&#xD;
3 sortFlag&#xD;
from stvterm&#xD;
where stvterm_code between '201009' and fy_stvterm_next(fy_stvterm_current('STANDARD', 'SICAS_TERM_TYPE', 'NEXT'), 'STANDARD')&#xD;
and stvterm_code &amp;amp;lt;&amp;amp;gt; fy_stvterm_next(fy_stvterm_current('STANDARD', 'SICAS_TERM_TYPE', 'NEXT'), 'STANDARD')&#xD;
--and substr(stvterm_code, 5, 2) not in ('00', '06') -- to be localized&#xD;
&#xD;
order by 3, 1 desc&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;&lt;/Choices&gt;&#xD;
     &lt;ChooseField&gt;code&lt;/ChooseField&gt;&#xD;
     &lt;Columns&gt;"code,,,WideString,0","description,,,WideString,0","SORTFLAG,,,BCD,0"&lt;/Columns&gt;&#xD;
     &lt;FreeType&gt;No&lt;/FreeType&gt;&#xD;
     &lt;Height&gt;21&lt;/Height&gt;&#xD;
     &lt;Left&gt;104&lt;/Left&gt;&#xD;
     &lt;ParentFont&gt;Yes&lt;/ParentFont&gt;&#xD;
     &lt;Required&gt;Yes&lt;/Required&gt;&#xD;
     &lt;ShowField&gt;description&lt;/ShowField&gt;&#xD;
     &lt;SqlEntries&gt;Yes&lt;/SqlEntries&gt;&#xD;
     &lt;TabOrder&gt;8&lt;/TabOrder&gt;&#xD;
     &lt;TabStop&gt;Yes&lt;/TabStop&gt;&#xD;
     &lt;Top&gt;160&lt;/Top&gt;&#xD;
     &lt;VariableName&gt;ddTerm_Advr&lt;/VariableName&gt;&#xD;
     &lt;Width&gt;169&lt;/Width&gt;&#xD;
    &lt;/control&gt;&#xD;
    &lt;control Type="MultiColumn"&gt;&#xD;
     &lt;Align&gt;None&lt;/Align&gt;&#xD;
     &lt;AutoSelect&gt;No&lt;/AutoSelect&gt;&#xD;
     &lt;AutoSizeColumn&gt;0&lt;/AutoSizeColumn&gt;&#xD;
     &lt;Choices&gt;&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select  distinct&#xD;
        "HomeDept",&#xD;
        "AdvisorName",&#xD;
        "DeptTerm",&#xD;
        "Term",&#xD;
        "Status",&#xD;
        "Pidm",&#xD;
        "BannerId",&#xD;
        "HomeInd"&#xD;
&#xD;
from    (&#xD;
        select  inst.sibinst_pidm as "Pidm",&#xD;
                f_format_name(inst.sibinst_pidm, 'LFMI') as "AdvisorName",&#xD;
                f_getspridenid(inst.sibinst_pidm) as "BannerId",&#xD;
                inst.sibinst_term_code_eff as "Term",&#xD;
                inst.sibinst_fcst_code as "Status",&#xD;
                coalesce(homedept, 'No Dept Assigned') as "HomeDept",&#xD;
                deptterm as "DeptTerm",&#xD;
                sirdpcl_home_ind as "HomeInd"&#xD;
&#xD;
        from    sibinst inst,&#xD;
                (&#xD;
                select (select dept.stvdept_desc from stvdept dept where dept.stvdept_code = pcl.sirdpcl_dept_code) as homedept,&#xD;
                        pcl.sirdpcl_term_code_eff as deptterm,&#xD;
                        pcl.sirdpcl_pidm,&#xD;
                        max(pcl.sirdpcl_home_ind) over (partition by pcl.sirdpcl_pidm, pcl.sirdpcl_dept_code) as sirdpcl_home_ind&#xD;
                from sirdpcl pcl&#xD;
                where pcl.sirdpcl_term_code_eff = (select max(cl.sirdpcl_term_code_eff)&#xD;
                                                 from sirdpcl cl&#xD;
                                                 where cl.sirdpcl_pidm = pcl.sirdpcl_pidm&#xD;
                                                 and cl.sirdpcl_term_code_eff &amp;amp;lt;= :ddTerm_Advr.code&#xD;
                                                 )&#xD;
              --  and pcl.sirdpcl_home_ind = 'Y'&#xD;
&#xD;
                ) dept&#xD;
&#xD;
        where   inst.sibinst_pidm = dept.sirdpcl_pidm (+)&#xD;
&#xD;
        and     inst.sibinst_term_code_eff = (select max(nst.sibinst_term_code_eff)&#xD;
                                              from sibinst nst&#xD;
                                              where nst.sibinst_pidm = inst.sibinst_pidm&#xD;
                                              and nst.sibinst_term_code_eff &amp;amp;lt;= :ddTerm_Advr.code&#xD;
                                              )&#xD;
        and     inst.sibinst_advr_ind = 'Y'&#xD;
        )&#xD;
where   ( :ddDeptAdv.code is null or "HomeDept" = :ddDeptAdv.desc)&#xD;
and     ( :cboxAdvrActive = 0 or "Status" = 'AC')&#xD;
and     ( :ddAdvr.pidm is null or "Pidm" = :ddAdvr.pidm)&#xD;
&#xD;
order by&#xD;
        1, 2&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;&lt;/Choices&gt;&#xD;
