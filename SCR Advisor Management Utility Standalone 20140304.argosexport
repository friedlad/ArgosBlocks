    <Data>&lt;datablock&gt;&#xD;
 &lt;information&gt;&#xD;
  &lt;description&gt;Argos Object File&lt;/description&gt;&#xD;
  &lt;development&gt;Argos Builder Version 4.2.5 Build 659&lt;/development&gt;&#xD;
  &lt;company&gt;Evisions, Inc.&lt;/company&gt;&#xD;
  &lt;date&gt;Wed, 26 Feb 2014 11:40:03 -0500&lt;/date&gt;&#xD;
 &lt;/information&gt;&#xD;
 &lt;query&gt;&#xD;
  &lt;ADOOptions/&gt;&#xD;
 &lt;/query&gt;&#xD;
 &lt;parameter_queries&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;TAB1,,WideString,19&amp;quot;,&amp;quot;TAB2,,WideString,22&amp;quot;,&amp;quot;TAB3,,WideString,21&amp;quot;,&amp;quot;TAB4,,WideString,4&amp;quot;,&amp;quot;TAB5,,WideString,2&amp;quot;,&amp;quot;TAB6,,WideString,2&amp;quot;,&amp;quot;TAB7,,WideString,2&amp;quot;,&amp;quot;TAB8,,WideString,2&amp;quot;,&amp;quot;TAB9,,WideString,2&amp;quot;" VariableName="sql_Tabs" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select  'Advisor Mass Update' as tab1,&amp;#xA;        'End Advisee Assignment' as tab2,&amp;#xA;        'Advisor - Change Dept' as tab3,&amp;#xA;        'Help' as tab4,&amp;#xA;        '' as tab5,&amp;#xA;        '' as tab6,&amp;#xA;        '' as tab7,&amp;#xA;        '' as tab8,&amp;#xA;        '' as tab9&amp;#xA;from    dual&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="" VariableName="sql_SwitchAdvr" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;declare&amp;#xA;v_return varchar2(1000);&amp;#xA;v_pidm number;&amp;#xA;&amp;#xA;cursor  c_pidm is&amp;#xA;select  distinct&amp;#xA;        sgbstdn_pidm&amp;#xA;from    sgbstdn&amp;#xA;where   sgbstdn_pidm = :mcStuFrom.pidm;&amp;#xA;&amp;#xA;begin&amp;#xA;v_return := 'Y';&amp;#xA;&amp;#xA;&amp;#xA;  open c_pidm;&amp;#xA;      loop&amp;#xA;          fetch c_pidm into v_pidm;&amp;#xA;          exit when c_pidm%notfound;&amp;#xA;          --dealing with a specific advisor type or not&amp;#xA;          select  f_sr_switchadvisors(:mcAdvrFrom.PIDM, :mcAdvrTo.PIDM, 'N', 'N', 'N', 'N',&amp;#xA;                                      'Y', v_pidm, fy_stvterm_current('STANDARD', 'SICAS_TERM_TYPE', 'NEXT')) &amp;quot;switchAdvr&amp;quot;&amp;#xA;          into v_return&amp;#xA;          from dual&amp;#xA;          where :btnMoveAdvisees is not null;&amp;#xA;&amp;#xA;      end loop;&amp;#xA;  close c_pidm;&amp;#xA;&amp;#xA;&amp;#xA;end;&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="" VariableName="sqlSwitchAdvDept" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;declare&amp;#xA;v_return varchar2(1000);&amp;#xA;v_pidm number;&amp;#xA;v_term varchar2(8);&amp;#xA;v_deptcode varchar2(25);&amp;#xA;v_homeind varchar2(10);&amp;#xA;v_percentage number;&amp;#xA;&amp;#xA;cursor  c_pidm is&amp;#xA;select  distinct&amp;#xA;        sibinst_pidm as pidm,&amp;#xA;        deptterm,&amp;#xA;        deptcode,&amp;#xA;        homeind,&amp;#xA;        percentage&amp;#xA;from    (&amp;#xA;        select  inst.sibinst_pidm,&amp;#xA;                dept.*&amp;#xA;&amp;#xA;        from    sibinst inst,&amp;#xA;                (&amp;#xA;                select (select  dept.stvdept_desc from stvdept dept where dept.stvdept_code = pcl.sirdpcl_dept_code) as homedept,&amp;#xA;                                pcl.sirdpcl_pidm as pidm,&amp;#xA;                                pcl.sirdpcl_term_code_eff as deptterm,&amp;#xA;                                pcl.sirdpcl_dept_code as deptcode,&amp;#xA;                                pcl.sirdpcl_coll_code as collcode,&amp;#xA;                                max(pcl.sirdpcl_home_ind) over (partition by pcl.sirdpcl_pidm, pcl.sirdpcl_dept_code) as homeind,&amp;#xA;                                pcl.sirdpcl_percentage as percentage,&amp;#xA;                                pcl.sirdpcl_activity_date as activitydate&amp;#xA;                        from    sirdpcl pcl&amp;#xA;                        where   pcl.sirdpcl_term_code_eff = (select max(cl.sirdpcl_term_code_eff)&amp;#xA;                                                         from sirdpcl cl&amp;#xA;                                                         where cl.sirdpcl_pidm = pcl.sirdpcl_pidm&amp;#xA;                                                         and cl.sirdpcl_term_code_eff &amp;amp;lt;= :ddTerm_Advr.code&amp;#xA;                                                         )&amp;#xA;                       -- and     pcl.sirdpcl_home_ind = 'Y'&amp;#xA;                ) dept&amp;#xA;&amp;#xA;        where   inst.sibinst_pidm = dept.pidm (+)&amp;#xA;&amp;#xA;        and     inst.sibinst_term_code_eff = (select max(nst.sibinst_term_code_eff)&amp;#xA;                                              from sibinst nst&amp;#xA;                                              where nst.sibinst_pidm = inst.sibinst_pidm&amp;#xA;                                              and nst.sibinst_term_code_eff &amp;amp;lt;= :ddTerm_Advr.code&amp;#xA;                                              )&amp;#xA;        and     inst.sibinst_advr_ind = 'Y'&amp;#xA;        )&amp;#xA;where   sibinst_pidm = :mcAdvr.Pidm&amp;#xA;and     :btnAdvDeptMove is not null;&amp;#xA;&amp;#xA;begin&amp;#xA;v_return := 'Y';&amp;#xA;&amp;#xA;&amp;#xA;    open c_pidm;&amp;#xA;        loop&amp;#xA;            fetch c_pidm into v_pidm, v_term, v_deptcode, v_homeind, v_percentage;&amp;#xA;            exit when c_pidm%notfound;&amp;#xA;            --dealing with a specific advisor type or not&amp;#xA;            case&amp;#xA;                when v_term = :ddTerm_Advr.code then --perform update&amp;#xA;                    update sirdpcl&amp;#xA;                    set sirdpcl_dept_code = :ddDeptAdvTo.code,&amp;#xA;                        sirdpcl_home_ind = decode(:ddAdvHomeInd.code, 'h', 'Y', 'e', v_homeind, 'z', 'N'),&amp;#xA;                        sirdpcl_activity_date = sysdate&amp;#xA;                    where sirdpcl_pidm = v_pidm&amp;#xA;                    and sirdpcl_term_code_eff = v_term&amp;#xA;                    and sirdpcl_dept_code = v_deptcode&amp;#xA;                    and :btnAdvDeptMove is not null;&amp;#xA;                else --insert new record&amp;#xA;                    insert into sirdpcl&amp;#xA;                                (sirdpcl_pidm, sirdpcl_term_code_eff, sirdpcl_coll_code, sirdpcl_dept_code,&amp;#xA;                                 sirdpcl_home_ind, sirdpcl_percentage, sirdpcl_activity_date)&amp;#xA;                    select   v_pidm,&amp;#xA;                             :ddTerm_Advr.code,&amp;#xA;                             '', --as coll code&amp;#xA;                             :ddDeptAdvTo.code,&amp;#xA;                             decode(:ddAdvHomeInd.code, 'h', 'Y', 'e', v_homeind), --home ind&amp;#xA;                             v_percentage,&amp;#xA;                             sysdate&amp;#xA;                    from     dual&amp;#xA;                    where    :btnAdvDeptMove is not null;&amp;#xA;            end case;&amp;#xA;&amp;#xA;        end loop;&amp;#xA;    close c_pidm;&amp;#xA;&amp;#xA;end;&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;selBannerId,,BCD,4&amp;quot;" VariableName="sql_SelByBannerId" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select fy_getpidm(upper(:txtBannerIdTerm)) &amp;quot;selBannerId&amp;quot; from dual&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="" VariableName="sql_EndAdvisor" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;begin&amp;#xA;--&amp;#xA;--  Terminating Advisor assignments involves inserting a new, empty record&amp;#xA;--  for an appropriate semester following the end of the student's association&amp;#xA;--  here (dismissal, graduation, becoming Inactive)&amp;#xA;--&amp;#xA;--  Problems happen when we would like to use a term as the final term, but&amp;#xA;--  it already has active assignments.&amp;#xA;--&amp;#xA;--&amp;#xA;-- The Graduated group - the GD goes onto a SGBSTDN with a term&amp;#xA;-- following the graduation term. Hopefully, if the person is&amp;#xA;-- re-enrolled, the GD has been manually changed to 'AS'&amp;#xA;--&amp;#xA;insert  into sgradvr&amp;#xA;select  distinct&amp;#xA;        pidm,&amp;#xA;        --if the closeout term is less or equal to the max advr term&amp;#xA;        --we add another term to close out the advisor record.&amp;#xA;        case&amp;#xA;            when max_advr_term = 999999 then fy_stvterm_current('STANDARD', 'SICAS_TERM_TYPE', 'NEXT')&amp;#xA;            when closeout_term_code &amp;amp;lt;= max_advr_term then fy_stvterm_next(max_advr_term)&amp;#xA;            else&amp;#xA;                closeout_term_code&amp;#xA;        end,&amp;#xA;        null,&amp;#xA;        null,&amp;#xA;        null,&amp;#xA;        sysdate&amp;#xA;&amp;#xA;from    (&amp;#xA;        --for students no longer associated here...&amp;#xA;        select  distinct&amp;#xA;                tdn.sgbstdn_pidm as pidm,&amp;#xA;                'Inactive' as rectype,&amp;#xA;                tdn.sgbstdn_stst_code as status,&amp;#xA;                tdn.sgbstdn_term_code_eff as term_code,&amp;#xA;                tdn.sgbstdn_levl_code as levl,&amp;#xA;                tdn.sgbstdn_styp_code as stype,&amp;#xA;                dvr.sgradvr_advr_code,&amp;#xA;                dvr.sgradvr_advr_pidm,&amp;#xA;                tdn.sgbstdn_term_code_eff as closeout_term_code,&amp;#xA;                dvr.sgradvr_term_code_eff as max_advr_term&amp;#xA;        from    sgbstdn tdn,&amp;#xA;                sgradvr dvr,&amp;#xA;                stvstst vstst&amp;#xA;        where   tdn.rowid = f_get_sgbstdn_rowid(tdn.SGBSTDN_PIDM, tdn.SGBSTDN_TERM_CODE_EFF)&amp;#xA;                /* tdn.sgbstdn_term_code_eff = (select max(n.sgbstdn_term_code_eff)&amp;#xA;                                              from sgbstdn n&amp;#xA;                                              where tdn.sgbstdn_pidm = n.sgbstdn_pidm)*/&amp;#xA;        and     tdn.SGBSTDN_STST_CODE = vstst.STVSTST_CODE (+)&amp;#xA;        and     vstst.STVSTST_CSTS_CODE in ('REMOVED', 'AWARDED', 'DENIED')&amp;#xA;                -- sgbstdn_stst_code IN ('GD','AK', 'DA', 'WD', 'IS', 'NS') -- replaced by previous 2 lines&amp;#xA;        and     tdn.sgbstdn_pidm = dvr.sgradvr_pidm&amp;#xA;        and     dvr.sgradvr_advr_pidm is not null&amp;#xA;        and     dvr.sgradvr_term_code_eff = (select max(r.sgradvr_term_code_eff)&amp;#xA;                                             from sgradvr r&amp;#xA;                                             where r.sgradvr_pidm = dvr.sgradvr_pidm)&amp;#xA;        and     not exists (&amp;#xA;                   select *&amp;#xA;                   from sfrstcr, stvrsts&amp;#xA;                   where sfrstcr_pidm = tdn.sgbstdn_pidm&amp;#xA;                   and sfrstcr_rsts_code = stvrsts_code&amp;#xA;                   and stvrsts_incl_sect_enrl = 'Y'&amp;#xA;                   and sfrstcr_term_code &amp;amp;gt;= fy_stvterm_current('STANDARD', 'SICAS_TERM_TYPE', 'NEXT')) --dbl_check that not currently registered&amp;#xA;&amp;#xA;    UNION&amp;#xA;&amp;#xA;        --This is for ACTIVE grad students previously under-grad and we need to&amp;#xA;        --close out the advisor record if that record belonged to the under-grad record.&amp;#xA;        select  distinct&amp;#xA;                stdn.sgbstdn_pidm as pidm,&amp;#xA;                'Active Grad' as rectype,&amp;#xA;                stdn.sgbstdn_stst_code as status,&amp;#xA;                stdn.sgbstdn_term_code_eff as term_code,&amp;#xA;                stdn.sgbstdn_levl_code as levl,&amp;#xA;                stdn.sgbstdn_styp_code as stype,&amp;#xA;                advr.sgradvr_advr_code,&amp;#xA;                advr.sgradvr_advr_pidm,&amp;#xA;                maxug.max_ug_term_code_eff as closeout_term_code,&amp;#xA;                advr.sgradvr_term_code_eff as max_advr_term&amp;#xA;        from    sgbstdn stdn,&amp;#xA;                sgradvr advr,&amp;#xA;                stvstst va, -- for determining campus non-specific active status&amp;#xA;                (select distinct n.sgbstdn_pidm,&amp;#xA;                        min(n.sgbstdn_term_code_eff) over (partition by sgbstdn_pidm) as term_code_eff_min,&amp;#xA;                        max(n.sgbstdn_term_code_eff) over (partition by sgbstdn_pidm) as term_code_eff_max&amp;#xA;                from    sgbstdn n,&amp;#xA;                        stvstst v&amp;#xA;                where   n.SGBSTDN_LEVL_CODE = :lbGraduateCodes.Level_Code&amp;#xA;                                        /* Previously (before Standalone version:&amp;#xA;                                           f_sz_isGradStudent(n.SGBSTDN_LEVL_CODE, n.SGBSTDN_PIDM, n.SGBSTDN_TERM_CODE_EFF) = 1      -- the SCR way */&amp;#xA;                                        /* Previously to determine Grad level:&amp;#xA;                                           n.sgbstdn_levl_code = 'GR' */&amp;#xA;                  and   n.SGBSTDN_STST_CODE = v.STVSTST_CODE (+)&amp;#xA;                  and   v.STVSTST_CSTS_CODE is null               -- campus non-specific way to determine Active status&amp;#xA;                                        /* Previously to determine Active Status:&amp;#xA;                                           n.sgbstdn_stst_code = 'AS' */&amp;#xA;                ) mdn, --this gets the min and max only for the GR record&amp;#xA;                (select distinct d.sgbstdn_pidm,&amp;#xA;                        max(sgbstdn_term_code_eff) as max_term_code_eff&amp;#xA;                from    sgbstdn d&amp;#xA;                group by d.sgbstdn_pidm&amp;#xA;                )maxdm, --this gets the max overall, combined with the above we ensure we are dealing only w/ GR's as the last record&amp;#xA;                (select distinct t.sgbstdn_pidm,&amp;#xA;                        max(t.sgbstdn_term_code_eff) as max_ug_term_code_eff&amp;#xA;                from    sgbstdn t&amp;#xA;                where   t.SGBSTDN_LEVL_CODE = :lbUndergradCodes.Level_Code&amp;#xA;                                        /* Previously (before Standalone version:&amp;#xA;                                           f_sz_isUndergrad(t.SGBSTDN_LEVL_CODE, t.SGBSTDN_PIDM, t.SGBSTDN_TERM_CODE_EFF) = 1      -- the SCR way */&amp;#xA;                                        /* Previously to determine undergrad level:&amp;#xA;                                           t.sgbstdn_levl_code = 'UG' */&amp;#xA;                group by t.sgbstdn_pidm&amp;#xA;                )maxug --making sure a UG record existed before date will be used because sometimes a record exists in&amp;#xA;                       --advr table where the date is between the last UG and 1st GR (eg pidm: 7420)&amp;#xA;&amp;#xA;        where   stdn.sgbstdn_pidm = mdn.sgbstdn_pidm&amp;#xA;        and     stdn.sgbstdn_pidm = maxdm.sgbstdn_pidm&amp;#xA;        and     stdn.sgbstdn_pidm = advr.sgradvr_pidm&amp;#xA;          and   stdn.SGBSTDN_STST_CODE = va.STVSTST_CODE (+)&amp;#xA;          and   va.STVSTST_CSTS_CODE is null               -- campus non-specific way to determine Active status, also innner join with STVSTST&amp;#xA;                                        /* Previously to determine Active Status:&amp;#xA;                                           stdn.sgbstdn_stst_code = 'AS' */&amp;#xA;        and     stdn.SGBSTDN_LEVL_CODE = :lbGraduateCodes.Level_Code&amp;#xA;                                        /* Previously (before Standalone version:&amp;#xA;                                           f_sz_isGradStudent(stdn.SGBSTDN_LEVL_CODE, stdn.SGBSTDN_PIDM, stdn.SGBSTDN_TERM_CODE_EFF) = 1      -- the SCR way */&amp;#xA;                                        /* Previously to determine Grad level:&amp;#xA;                                           stdn.sgbstdn_levl_code = 'GR' */&amp;#xA;        and     stdn.sgbstdn_term_code_eff = mdn.term_code_eff_max&amp;#xA;        and     stdn.sgbstdn_term_code_eff = maxdm.max_term_code_eff&amp;#xA;        and     stdn.sgbstdn_pidm = maxug.sgbstdn_pidm&amp;#xA;        and     advr.sgradvr_term_code_eff = (select max(dvr.sgradvr_term_code_eff) from sgradvr dvr where dvr.sgradvr_pidm = advr.sgradvr_pidm)&amp;#xA;        and     advr.sgradvr_advr_pidm is not null&amp;#xA;        and     advr.sgradvr_term_code_eff &amp;amp;lt;= maxug.max_ug_term_code_eff&amp;#xA;        ) mainall&amp;#xA;&amp;#xA;where   pidm = :mcStudentsTerm.PIDM&amp;#xA;and     term_code = :ddTermTerm.Term&amp;#xA;and     :lbUndergradCodes.Level_Code is not null&amp;#xA;and     :lbGraduateCodes.Level_Code is not null&amp;#xA;and     :buttEndAdvisorTerm is not null;&amp;#xA;commit;&amp;#xA;--&amp;#xA;--  Finally, the no show group - their entry date will match the term code&amp;#xA;--  and they will have no registrations. These we can simply delete.&amp;#xA;--&amp;#xA;delete&amp;#xA;from    sgradvr a&amp;#xA;where   exists (&amp;#xA;                select  sgbstdn_pidm&amp;#xA;                from    sgbstdn,&amp;#xA;                        stvstst v&amp;#xA;                where   sgbstdn_term_code_eff = :ddTermTerm.Term&amp;#xA;                  and   SGBSTDN_STST_CODE = v.STVSTST_CODE (+)&amp;#xA;                  and   v.STVSTST_CSTS_CODE is null               -- campus non-specific way to determine Active status&amp;#xA;                                        /* Previously to determine Active Status:&amp;#xA;                                        and     sgbstdn_stst_code in ('AS') */&amp;#xA;                and     sgbstdn_term_code_eff = sgradvr_term_code_eff&amp;#xA;                and     sgbstdn_term_code_eff = sgbstdn_term_code_admit&amp;#xA;                and     sgradvr_term_code_eff = (&amp;#xA;                                                select max (sgradvr_term_code_eff)&amp;#xA;                                                from sgradvr&amp;#xA;                                                where sgradvr_pidm = a.sgradvr_pidm&amp;#xA;                                                )&amp;#xA;                and sgradvr_advr_pidm is not null&amp;#xA;                and sgradvr_pidm = sgbstdn_pidm&amp;#xA;                and sgbstdn_term_code_eff = (&amp;#xA;                                            select max(sgbstdn_term_code_eff)&amp;#xA;                                            from sgbstdn&amp;#xA;                                            where sgbstdn_pidm = sgradvr_pidm&amp;#xA;                                            )&amp;#xA;                and not exists (&amp;#xA;                                select *&amp;#xA;                                from sfrstcr, stvrsts&amp;#xA;                                where sfrstcr_pidm = sgbstdn_pidm&amp;#xA;                                and sfrstcr_rsts_code = stvrsts_code&amp;#xA;                                and stvrsts_incl_sect_enrl = 'Y'&amp;#xA;                                and sfrstcr_term_code &amp;amp;gt;= :ddTermTerm.Term&amp;#xA;                                )&amp;#xA;                and not exists (&amp;#xA;                                select sgradvr_pidm&amp;#xA;                                from sgradvr&amp;#xA;                                where sgradvr_pidm = a.sgradvr_pidm&amp;#xA;                                and sgradvr_term_code_eff &amp;amp;lt; a.sgradvr_term_code_eff)&amp;#xA;                                )&amp;#xA;and     :lbUndergradCodes.Level_Code is not null&amp;#xA;and     :lbGraduateCodes.Level_Code is not null&amp;#xA;                and :buttEndAdvisorTerm is not null;&amp;#xA;commit;&amp;#xA;end;&amp;lt;/Lines&amp;gt;&amp;lt;Query Validated=&amp;quot;False&amp;quot;&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
 &lt;/parameter_queries&gt;&#xD;
